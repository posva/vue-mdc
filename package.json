{
  "name": "vue-mdl",
  "version": "0.2.3",
  "description": "Reusable Vue components using Material Design Lite",
  "main": "dist/vue-mdl.js",
  "scripts": {
    "test": "npm run test:unit && npm run test:e2e",
    "test:e2e": "./build/e2e.sh",
    "test:unit": "./build/unit.sh",
    "test:chrome": "./build/e2e.sh chrome",
    "test:firefox": "./build/e2e.sh firefox",
    "build": "npm run build:dev && npm run build:prod",
    "build:dev": "webpack --progress --bail --config build/webpack.dev.config.js",
    "build:prod": "webpack -p --progress --bail --config build/webpack.prod.config.js",
    "dev": "webpack-dev-server --config build/webpack.unit.config.js --port 8088",
    "dev:test": "webpack-dev-server --config build/webpack.test.config.js --port 8088",
    "dev:doc": "webpack-dev-server --config build/webpack.doc.config.js --port 8088",
    "doc": "webpack -p --progress --bail --config build/webpack.gh-pages.config.js"
  },
  "keywords": [
    "vue",
    "mdl",
    "ui",
    "desgin",
    "google",
    "material",
    "lite",
    "bootstrap",
    "vuestrap",
    "vuemdl",
    "front",
    "normalize",
    "css"
  ],
  "author": "posva",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^1.0.10"
  },
  "devDependencies": {
    "bluebird": "^3.0.5",
    "chai": "^3.4.1",
    "chai-jquery": "^2.0.0",
    "chromedriver": "^2.20.0",
    "coffee-loader": "^0.7.2",
    "coffee-script": "^1.10.0",
    "css-loader": "^0.23.0",
    "highlight.js": "^9.0.0",
    "html-loader": "^0.4.0",
    "http-server": "^0.8.5",
    "jade": "^1.11.0",
    "jquery": "^2.1.4",
    "lodash": "^3.10.1",
    "material-design-lite": "^1.0.6",
    "mocha": "^2.3.4",
    "mocha-phantomjs": "^4.0.1",
    "nightwatch": "^0.8.6",
    "node-sass": "^3.4.2",
    "sass-loader": "^3.1.2",
    "selenium-server": "^2.48.2",
    "style-loader": "^0.13.0",
    "stylus-loader": "^1.4.2",
    "template-html-loader": "0.0.3",
    "url-loader": "^0.5.7",
    "vue": "^1.0.10",
    "vue-hot-reload-api": "^1.2.1",
    "vue-html-loader": "^1.0.0",
    "vue-loader": "^7.1.4",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/posva/vue-mdl.git"
  },
  "bugs": {
    "url": "https://github.com/posva/vue-mdl/issues"
  },
  "homepage": "https://github.com/posva/vue-mdl#readme"
}
