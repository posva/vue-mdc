{
  "name": "vue-mdl",
  "version": "1.0.0-alpha.4",
  "description": "Reusable Vue components using Material Design Lite",
  "main": "dist/vue-mdl.js",
  "scripts": {
    "test": "npm run lint && npm run test:unit",
    "start": "NODE_ENV=dev node build/dev-server.js",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "test:unit": "karma start test/unit/karma.conf.js --single-run",
    "build": "npm run build:dev && npm run build:prod",
    "build:prod": "webpack --bail --progress --hide-modules --config build/webpack.dist.prod.conf.js",
    "build:dev": "webpack --bail --progress --hide-modules --config build/webpack.dist.dev.conf.js",
    "dev": "karma start test/unit/karma.dev.conf.js",
    "dev:integration": "node build/dev-server.js",
    "dev:doc": "NODE_ENV=doc node build/dev-server.js",
    "doc": "webpack -p --progress --bail --config build/webpack.gh-pages.conf.js"
  },
  "keywords": [
    "vue",
    "mdl",
    "ui",
    "desgin",
    "google",
    "material",
    "lite",
    "bootstrap",
    "vuestrap",
    "vuemdl",
    "front",
    "normalize",
    "css"
  ],
  "author": "posva",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-runtime": "^6.18.0",
    "chai": "^3.5.0",
    "chromedriver": "2.24.1",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.8.1",
    "eslint-config-standard": "^6.2.0",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-html": "^1.5.5",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-standard": "^2.0.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "function-bind": "^1.1.0",
    "highlight.js": "^9.7.0",
    "html-webpack-plugin": "^2.24.0",
    "http-proxy-middleware": "^0.17.2",
    "inject-loader": "^3.0.0-beta2",
    "isparta-loader": "^2.0.0",
    "jade": "^1.11.0",
    "karma": "^1.3.0",
    "karma-chai-dom": "^1.1.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sinon-chai": "^1.2.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.8.0",
    "lodash": "^4.16.4",
    "lolex": "^1.5.1",
    "material-design-lite": "^1.2.1",
    "mocha": "^3.1.2",
    "nightwatch": "^0.9.8",
    "node-sass": "^3.10.1",
    "normalize.css": "^5.0.0",
    "ora": "^0.3.0",
    "phantomjs-prebuilt": "^2.1.13",
    "sass-loader": "^4.0.2",
    "selenium-server": "^3.0.1",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "url-loader": "^0.5.7",
    "vue": "latest",
    "vue-hot-reload-api": "^2.0.6",
    "vue-html-loader": "^1.2.3",
    "vue-loader": "latest",
    "vue-router": "^2.0.1",
    "vue-style-loader": "^1.0.0",
    "vue-template-compiler": "^2.1.6",
    "vue-transfer-dom": "^1.1.0",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.0",
    "webpack-merge": "^0.15.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/posva/vue-mdl.git"
  },
  "bugs": {
    "url": "https://github.com/posva/vue-mdl/issues"
  },
  "homepage": "https://github.com/posva/vue-mdl#readme",
  "dependencies": {
    "focus-trap": "^2.1.0"
  }
}
